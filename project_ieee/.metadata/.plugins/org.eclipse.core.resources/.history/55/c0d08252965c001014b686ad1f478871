#include "BUZZ.h"

#define Buzzr_pin	Pin_5
#define Buzzr_port	Port_B


void Buzzer_init() {
	GPIO_vSetPinDirection(Buzzr_port, Buzzr_pin, Pin_Output);
}

void Buzzer_ON() {
	GPIO_vSetPinValue(Buzzr_port, Pin_5, Pin_High);
}

void Buzzer_OFF() {
	GPIO_vSetPinValue(Buzzr_port, Pin_5, Pin_High);
}
